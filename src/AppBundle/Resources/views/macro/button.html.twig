{% macro create(name, title, href, onclick, class, data) %}
    <a class="btn btn_{{ name }} {{ class }}" {% if data %}data-type={{ data }}{% endif %} {% if href %}href="{{ href }}"{% endif %} title="{{ title }}" onclick="{% if onclick == 'Nav' %}return Nav.get('{{ href }}');{% else %}{{ onclick }}{% endif %}"><span>{{ title }}</span></a>
{% endmacro %}